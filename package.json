{
	"name": "foma-highlighting",
	"displayName": "Foma Highlighting",
	"description": "Syntax highlighting for .lexc and .foma files.",
	"version": "1.0.0",
	"license": "GPLv3",
	"engines": {
		"vscode": "^1.71.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/scott-parkhill/foma-highlighting.git"
	},
	"publisher": "scott-parkhill",
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "lexc",
				"extensions": [
					".lexc"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "foma",
				"extensions": [
					".foma"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "lexc",
				"scopeName": "source.lexc",
				"path": "./syntaxes/lexc.tmLanguage.json"
			},
			{
				"language": "foma",
				"scopeName": "source.foma",
				"path": "./syntaxes/foma.tmLanguage.json"
			}
		]
	},
	"__metadata": {
		"id": "30d40429-3327-47a4-8b4d-4bb0ae06c7ff",
		"publisherDisplayName": "Scott Parkhill",
		"publisherId": "c608b32f-29fc-44bd-a728-a77e49201e9e",
		"isPreReleaseVersion": false
	}
}
